{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "checkJs": true,
    "noEmit": true,
    "allowJs": true,
    "lib": ["ES2020", "DOM"],
    "typeRoots": [
      "./node_modules/@types",
      "./JS-Slash-Runner/@types"
    ],
    "types": [
      "jquery",
      "lodash",
      "toastr"
    ]
  },
  "include": [
    "**/*.js",
    "./JS-Slash-Runner/@types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.min.js"
  ]
}